		"file-loader": "^4.0.0",
		"terser-webpack-plugin": "^1.3.0",
		"webpack": "^4.34.0",
		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.7.1"
	},
	"prettier": {
		"trailingComma": "es5",
		"useTabs": false,
		"tabWidth": 4,
		"semi": true,
